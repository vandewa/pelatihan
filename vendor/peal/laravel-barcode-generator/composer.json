{
    "name": "peal/laravel-barcode-generator",
    "description": "Bar code generate in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Moinuddin Chowdhury",
            "email": "moinuddin7@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "peal\\barcodegenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "peal\\barcodegenerator\\BarcodeServiceProvider"
            ],
            "aliases": {
                "BarCode": "peal\\BarcodeServiceProvider\\Facades\\BarCode"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "5.2.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*"
    }
}
